{
  "name": "ots-website",
  "version": "2026-01-10.1",
  "description": "OpenFront Tactical Suitcase documentation website",
  "type": "module",
  "scripts": {
    "sync": "rsync -av --delete --exclude='index.md' ../doc/user/ ./user/ && rsync -av --delete --exclude='index.md' ../doc/developer/ ./developer/",
    "dev": "npm run sync && vitepress dev",
    "build": "npm run sync && vitepress build",
    "preview": "vitepress preview"
  },
  "devDependencies": {
    "vitepress": "^1.0.0",
    "vue": "^3.4.0"
  }
}